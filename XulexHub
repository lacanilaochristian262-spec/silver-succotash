]]
local Players = game:GetService("Players")
local player = Players.LocalPlayer

local function blockAnimation(character)
    local humanoid = character:WaitForChild("Humanoid")
    
    humanoid.AnimationPlayed:Connect(function(track)
        if track.Animation.AnimationId == "rbxassetid://13927612951" then
            track:Stop(0)
        end
    end)
end

if player.Character then
    blockAnimation(player.Character)
end

player.CharacterAdded:Connect(blockAnimation)
        end
    })
end

