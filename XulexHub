    })

    local FESection = FETab:Section({
        Title = "FE Scripts List",
    })

    -- Function to create buttons faster
    local function AddFeButton(name, url, isAsync)
        FETab:Button({
            Title = name,
            Callback = function()
                if isAsync then
                    loadstring(game:HttpGetAsync(url))()
                else
                    loadstring(game:HttpGet(url))()
                end
            end
        })
        FETab:Space() -- Adds consistent spacing between buttons
