local WindUI = loadstring(game:HttpGet("https://github.com/Footagesus/WindUI/releases/latest/download/main.lua"))()

local Window = WindUI:CreateWindow({
    Title = "Xulex Hub | " .. game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name,
    Icon = "solar:folder-2-bold-duotone",
    Author = "by. basic exploiter",
    Folder = "MySuperHub",
    
    Size = UDim2.fromOffset(580, 460),
    MinSize = Vector2.new(560, 350),
    MaxSize = Vector2.new(850, 560),
    Transparent = true,
    Theme = "Dark",
    Resizable = true,
    SideBarWidth = 200,
    BackgroundImageTransparency = 0.42,
    HideSearchBar = true,
    ScrollBarEnabled = false,
    
    User = {
        Enabled = true,
        Anonymous = true,
        Callback = function()
            print("clicked")
        end,
    },
})

-- */  Home Dashboard Tab  /* --
do
    local HomeTab = Window:Tab({
        Title = "Home",
        Desc = "Main control center", 
        Icon = "solar:home-2-bold",
        IconColor = Grey,
        IconShape = "Square",
    })
    
    local DashboardSection = HomeTab:Section({
        Title = "Dashboard",
    })

    -- Statistics or Welcome Group
    local WelcomeGroup = HomeTab:Group({})
    
    WelcomeGroup:Section({
        Title = "Welcome back, " .. game.Players.LocalPlayer.DisplayName .. "!",
        Desc = "The script is currently active and running.",
        TextSize = 18,
    })

    HomeTab:Space()

    -- Example Dashboard Buttons
    local QuickActions = HomeTab:Group({
        Title = "Quick Actions"
    })

    QuickActions:Button({
        Title = "Check Connection",
        Icon = "wifi",
        Callback = function()
            WindUI:Notify({
                Title = "Network Status",
                Content = "Connection is stable.",
                Duration = 3
            })
        end
    })

    QuickActions:Space()

    QuickActions:Button({
        Title = "Copy Owners Profile Link",
        Icon = "link",
        Callback = function()
            setclipboard("https://www.roblox.com/users/8733716932/profile")
            WindUI:Notify({
                Title = "Success",
                Content = "Link copied to clipboard!",
                Duration = 3
            })
        end
    })
    
    HomeTab:Space()

    -- */ Warning Section /* --
    -- Using a Group with a title to make it look like an Alert Box
    local WarningGroup = HomeTab:Group({
        Title = "⚠️ Status Alert"
    })
    
    WarningGroup:Section({
        Title = "Some scripts are currently patched.",
        Desc = "Due to recent game updates, certain features may not function. Please wait for the next update.",
        TextSize = 15,
        -- We use a subtle red/orange tint if the UI allows, 
        -- otherwise standard text works perfectly.
    })

HomeTab:Space({ Columns = 2 })

    -- */ Beta Notice Section /* --
    local FooterSection = HomeTab:Section({
        Title = "Notice: This Hub is currently in Beta and will Add More Scripts.",
        TextSize = 14,
        TextTransparency = 0.5, -- Makes it look like a footer/subtle note
        FontWeight = Enum.FontWeight.Medium,
    })
end

